class Bus
{    
  void run()
{System.out.println("Bus is running");}  
}  
  
class Bike extends Bus{   
  void run(){System.out.println("Bike is running safely");}  
  
  public static void main(String args[]){  
  Bike obj = new Bike();//creating object  
  obj.run();//calling method  
  }  
}   
